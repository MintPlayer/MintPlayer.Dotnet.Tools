<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <PropertyGroup>
        <MintPlayerValueComparerGeneratorAttributeAssembly Condition="'$(MintPlayerValueComparerGeneratorAttributeAssembly)' == '' and Exists('$(MSBuildThisFileDirectory)..\analyzers\dotnet\cs\MintPlayer.ValueComparerGenerator.Attributes.dll')">$(MSBuildThisFileDirectory)..\analyzers\dotnet\cs\MintPlayer.ValueComparerGenerator.Attributes.dll</MintPlayerValueComparerGeneratorAttributeAssembly>
    </PropertyGroup>

    <PropertyGroup Condition="'$(MintPlayerValueComparerGeneratorAttributeAssembly)' != ''">
        <GetTargetPathDependsOn>$(GetTargetPathDependsOn);GetDependencyTargetPathsValueComparerGenerator</GetTargetPathDependsOn>
    </PropertyGroup>

    <Target Name="GetDependencyTargetPathsValueComparerGenerator" Condition="'$(MintPlayerValueComparerGeneratorAttributeAssembly)' != ''">
        <ItemGroup>
            <TargetPathWithTargetPlatformMoniker Include="$(MintPlayerValueComparerGeneratorAttributeAssembly)" IncludeRuntimeDependency="true" />
        </ItemGroup>
    </Target>

</Project>
